{
  "name": "storefront_backend",
  "version": "1.0.0",
  "description": "Udacity Storefront Backend Project",
  "main": "build/server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "lint": "eslint --fix src/* && echo 'Lint complete.'",
    "prettier": "prettier --config .prettierrc \"src/**/*.(js|ts|json)\" --write",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess \"node ./build/server.js\"",
    "test": "db-migrate --env test reset && set ENV=test&& npm run build && db-migrate --env test up && jasmine && db-migrate --env test reset",
    "build": "npx tsc"
  },
  "author": "Daniel Misik",
  "license": "ISC",
  "dependencies": {
    "@altostra/type-validations": "^2.6.3",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.40",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.16",
    "prettier": "^2.5.1",
    "supertest": "^6.2.3",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.7.3"
  }
}
